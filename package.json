  "scripts": {
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --parallel build",
    "start": "pnpm run prisma-db-push && pnpm run --parallel start",
    "pm2": "pm2 delete all || true && pnpm run prisma-db-push && pnpm run --parallel pm2 && pm2 logs",
    "postinstall": "pnpm run prisma-db-generate",
    "dev:backend": "rm -rf dist/backend && pnpm --filter ./apps/backend run dev",
    "dev:workers": "rm -rf dist/workers && pnpm --filter ./apps/workers run dev",
    "dev:frontend": "rm -rf dist/frontend && pnpm --filter ./apps/frontend run dev",
    "build:backend": "rm -rf dist/backend && pnpm --filter ./apps/backend run build",
    "build:workers": "rm -rf dist/workers && pnpm --filter ./apps/workers run build",
    "build:frontend": "rm -rf dist/frontend && pnpm --filter ./apps/frontend run build",
    "start:backend": "pnpm --filter ./apps/backend run start",
    "start:workers": "pnpm --filter ./apps/workers run start",
    "start:frontend": "pnpm --filter ./apps/frontend run start",
    "prisma-db-push": "pnpx prisma db push --schema ./packages/backend/database/prisma/schema.prisma",
    "prisma-db-generate": "pnpx prisma generate --schema ./packages/backend/database/prisma/schema.prisma"
  },
